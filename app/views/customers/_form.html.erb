<%= simple_form_for @customer do |f| %>
  <%= f.error_messages %>
  <%= f.input :name%>
  <%= f.input :street%>
  <%= f.input :city%>
  <%= f.input :state%>
  <%= f.input :zipcode%>
  <%= f.input :email%>
  <%= f.input :phone%>
  <%= f.input :mobile%>
  <%= f.input :active%>
  <%= f.button :submit %>
<% end %>
